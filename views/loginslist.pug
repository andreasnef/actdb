extends layout

block content
  h3. 
     Logins
  
  a(href="/home") Go back to Index
  
  table(id="loginsList")
      thead    
        tr#headtag
            th Date (UTC)
            th User
            th IP
            
      tbody        
            each login, i in collList
                tr
                    //td=moment(login.date).format('MMMM Do, YYYY, h:mm:ss a')
                    td=login.date
                    td=login.user
                    //td=login.date
                    td=login.ip
                    
  
  script.
    var table = $('#loginsList').DataTable();
 
    table
    .column( '0:visible')
    .order( 'des' )
    .draw();
    
