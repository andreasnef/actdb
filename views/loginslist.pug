extends layout

block content
  h3. 
     Logins
  
  a(href="/home") Go back to Index
  
  table(id="loginsList")
      thead    
        tr#headtag
            th Date (UTC)
            th User
            th IP
            
      tbody        
            each login, i in collList
                tr
                    td=moment(login.date).format('MMMM Do, YYYY, h:mm:ss a')
                    td=login.user
                    td=login.ip
                    
  
  script.
    $('#loginsList').DataTable( {
        paging: true
        } );        